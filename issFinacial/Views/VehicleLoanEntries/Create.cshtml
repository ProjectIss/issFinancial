                                </div>
                                <div class="form-group col-md-4">
                                    <label>Insurance Expity Date</label>
                                    @Html.TextBoxFor(m => m.insuranceExpityDate, "{0:yyyy-MM-dd}", new { @class = "form-control", type = "date" })
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="text-right">
                        <button type="submit" value="Create" class="btn btn-success">Submit</button>
                        @Html.ActionLink("Cancel", "Index", null, htmlAttributes: new { @class = "btn btn-primary" })
                    </div>
                </div>
            </div>

        }
    </div>
</div>
