    function addDaysToDate(days) {
        var mths = new Array("Jan", "Feb", "Mar",
            "Apr", "May", "Jun", "Jul", "Aug", "Sep",
            "Oct", "Nov", "Dec");

        var d = new Date();
        d.setHours(d.getHours() + (24 * days));

        var currD = d.getDate();
        var currM = d.getMonth();
        var currY = d.getFullYear();

        return mths[currM] + " " + currD + ", " + currY;
    }


    $(document).ready(function () {
        $("#customerId").change(function () {
            var name = $(this).find('option:selected').val();
            customerName(name);
        })
       
    });
    function customerName(name) {
              $.ajax({
            type: 'POST',
            url: '@Url.Content("~/VehicleLoanEntries/customerName")',
            dataType: 'json',
            data: { NAME: name },
                  success: function (result) {
                      console.log(result)
                var data = result;

                      $("#customerNameId").empty();
                      $("#phoneNo").empty();
                for (var i = 0; i < data.length; i++) {
                    $("#customerNameId").append("<option value='" + data[i].id + "'>" + data[i].customerName + "</option>");
                    $("#phoneNo").val(data[i].contactNo)
                    $("#age").val(data[i].Age)
                    $("#address").val(data[i].customerAddress)
                    $("#fatherName").val(data[i].FatherName)

                }
            },
            error: function (err) {
                console.log(err);
            }
        });
    };


</script>
